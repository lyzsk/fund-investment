<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.sichu.mapper.FundDividendTransactionMapper">
    <resultMap id="resultMap" type="cn.sichu.entity.FundDividendTransaction">
        <id property="id" column="id"/>
        <result property="code" column="code"/>
        <result property="applicationDate" column="application_date"/>
        <result property="transactionDate" column="transaction_date"/>
        <result property="confirmationDate" column="confirmation_date"/>
        <result property="settlementDate" column="settlement_date"/>
        <result property="amount" column="amount"/>
        <result property="share" column="share"/>
        <result property="dividendAmountPerShare" column="dividend_amount_per_share"/>
        <result property="tradingPlatform" column="trading_platform"/>
        <result property="status" column="status"/>
        <result property="mark" column="mark"/>
    </resultMap>

    <insert id="insertFundDividendTransaction" useGeneratedKeys="true" keyProperty="id">
        insert into fund_dividend_transaction
        (code, application_date, transaction_date, confirmation_date, settlement_date, amount, share,
        dividend_amount_per_share, trading_platform, status, mark)
        values
        (#{code}, #{applicationDate}, #{transactionDate}, #{confirmationDate}, #{settlementDate},
        #{amount}, #{share}, #{dividendAmountPerShare}, #{tradingPlatform}, #{status}, #{mark})
    </insert>
</mapper>